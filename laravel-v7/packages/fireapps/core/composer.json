{
    "name": "fireapps/core",
    "authors": [
        {
            "name": "LongHM",
            "email": "longhm@fireapps.vn"
        }
    ],
    "description": "A core package for Fireapps with laravel 5.8",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.5",
        "firebase/php-jwt": "^5.0",
        "facebook/graph-sdk": "^5.7",
        "facebook/php-business-sdk": "3.1.*"
    },
    "autoload": {
        "files": [
            "./helpers/common.php"
        ],
        "psr-4": {
            "Fireapps\\Core\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fireapps\\Core\\FireappsProvider"
            ]
        }
    }
}
